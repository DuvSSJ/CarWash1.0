@model List<CarWash1._0.Models.UsuarioModel>

<h2>Lista de Usuarios</h2>
<button class="btn btn-primary">
    <i class="fas fa-user-plus"></i> Nuevo
</button>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Correo Electronico</th>
            <th>Telefono</th>
            <th>Direccion</th>
            <th>Fecha Ingreso</th>
            <th>Fecha Salida</th>
            <th>Obser Renuncia</th>
            <th>Usuario</th>
            <th>Clave</th>
            <th>Estado</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.IdEmpleado</td>
                <td>@usuario.Nombre</td>
                <td>@usuario.Apellido</td>
                <td>@usuario.Correo</td>
                <td>@usuario.Telefono</td>
                <td>@usuario.Direccion</td>
                <td>@usuario.FechaIngreso</td>
                <td>@usuario.FechaSalida</td>
                <td>@usuario.ObservacionRenuncia</td>
                <td>@usuario.Usuario </td>
                <td>@usuario.Clave</td>
                <td>@usuario.Estado</td>

                <td>
                    <a class="btn btn-warning"><i class="fa-solid fa-pen-to-square"></i> </a>
                    <a class="btn btn-danger"><i class="fa-solid fa-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>
